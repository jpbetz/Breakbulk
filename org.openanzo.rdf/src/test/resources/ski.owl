<rdf:RDF
	xmlns:base="http://jastor.openanzo.org/testonts/"
	xmlns:preds="http://jastor.openanzo.org/testonts/predicates#"
	xmlns:classes="http://jastor.openanzo.org/testonts/classes#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:dc="http://purl.org/dc/elements/1.1/">
	
	<owl:Ontology rdf:about="http://jastor.openanzo.org/testonts/Ski">
		<rdfs:label>Ski Ontology</rdfs:label>
		<dc:creator>Ben Szekely</dc:creator>
		<dc:contributor>Joe Betz</dc:contributor>
		<owl:versionInfo>0.1</owl:versionInfo>
		<rdfs:comment>Ontology for skis</rdfs:comment>
	</owl:Ontology>

	<classes:TwinTip rdf:about="http://jastor.openanzo.org/testonts/Ski/TeneightyLab" />
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#Ski">
		<rdfs:comment>This class represents the base ontology class for describing skis</rdfs:comment>
		<rdfs:label>Ski Class</rdfs:label>
		<rdfs:seeAlso rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip" />
		<rdfs:seeAlso rdf:resource="http://jastor.openanzo.org/testonts/classes#PowderSki" />
		<dc:title>Ski Title</dc:title>
		<dc:creator>Ben Szekely</dc:creator>
		<dc:contributor>Joe Betz</dc:contributor>
		<owl:versionInfo>1.0</owl:versionInfo>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>					
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#manufacturer">
						<rdfs:comment>This property indicates the manufacturer of the ski</rdfs:comment>
						<rdfs:label>Manufacturer</rdfs:label>
						<dc:title>Manufacturer Title</dc:title>
						<dc:creator>Ben Szekely</dc:creator>
						<dc:contributor>Joe Betz</dc:contributor>
						<owl:versionInfo>1.0</owl:versionInfo>
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#model">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#coreConstruction">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>		

		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#availableLength">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
	
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#partnum">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>

		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="0">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#relatedPartnum">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>

		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#website">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="0">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#competesWith">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#mostSimilarTo">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#attribute">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#identifier">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>

		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="0">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#multiIdentifier">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>

		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#previousModel" />
				<owl:allValuesFrom rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski" />					
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<!-- tests a range that is an anonymous enumerated class -->
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#sidewall">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
						<rdfs:range>
							<owl:Class>
								<owl:oneOf rdf:parseType="Collection">
						   			<owl:Thing rdf:about="http://jastor.openanzo.org/testonts/individuals#Vertical" />
						    		<owl:Thing rdf:about="http://jastor.openanzo.org/testonts/individuals#Cap" />
						    		<owl:Thing rdf:about="http://jastor.openanzo.org/testonts/individuals#Angled" />
						  		</owl:oneOf>
							</owl:Class>
						</rdfs:range>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
			
	</owl:Class>

	<!-- single proprety to test what happens when I class has an allValuesFrom restriction
	     that is the same as the range -->
	<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#previousModel">
		<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
		<rdfs:range rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
	</owl:ObjectProperty>


	<!-- single property to see if it gets added to the same frame as the parent property -->
	<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#coreMaterial">
	 	<rdfs:subPropertyOf rdf:resource="http://jastor.openanzo.org/testonts/predicates#coreConstruction" />
	</owl:DatatypeProperty>
			
		
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#TwinTip">
		<rdfs:subClassOf rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski" />
		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#pipeOrPark">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>		
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#competesWith" />
				<owl:allValuesFrom rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip" />
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#attribute" />
				<owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#mostSimilarTo" />
				<owl:allValuesFrom rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip" />
			</owl:Restriction>
		</rdfs:subClassOf>		
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#designer" />
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#specialty" />
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<!-- this is to test duplicate property names -->
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/altnamespace/predicates#model">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#PowderSki">
		<rdfs:subClassOf rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski" />
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#flotation">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#PowderSki"/>
						<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#competesWith" />
				<owl:allValuesFrom rdf:resource="http://jastor.openanzo.org/testonts/classes#PowderSki" />
			</owl:Restriction>
		</rdfs:subClassOf>	
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#designer" />
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#specialty" />
			</owl:Restriction>
		</rdfs:subClassOf>				
	</owl:Class>	
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#FatTwin">
		<rdfs:subClassOf rdf:resource="http://jastor.openanzo.org/testonts/classes#PowderSki" />
	    <rdfs:subClassOf rdf:resource="http://jastor.openanzo.org/testonts/classes#TwinTip" />
	    <rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#pipeOrPark" />
				<owl:hasValue>park</owl:hasValue>					
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#relative">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#FatTwin"/>
						<rdfs:range>
							<owl:Class>
							    <owl:unionOf rdf:parseType="Collection">
							      <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#PowderSki"/>
							      <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
							    </owl:unionOf>
							</owl:Class>
						</rdfs:range>
					</owl:ObjectProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
	</owl:Class>
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#SpecialtySki">
		<owl:unionOf rdf:parseType="Collection">
		    <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#PowderSki"/>
		    <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
 		</owl:unionOf>
	</owl:Class>	
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#PipeSki">
		<owl:intersectionOf rdf:parseType="Collection">
		    <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
		    <owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#pipeOrPark" />
				<owl:hasValue>pipe</owl:hasValue>					
			</owl:Restriction>
 		</owl:intersectionOf>
	</owl:Class>
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#Snowboard">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#complimentBoard" />
				<owl:allValuesFrom rdf:resource="http://jastor.openanzo.org/testonts/classes#Snowboard" />					
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#preferredStance" />
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty rdf:resource="http://jastor.openanzo.org/testonts/predicates#isAlpine" />
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:minCardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#availableBoardLength">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Snowboard"/>
						<rdfs:subPropertyOf rdf:resource="http://jastor.openanzo.org/testonts/predicates#availableLength" />
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
		<rdfs:subClassOf>
			<owl:Restriction owl:cardinality="1">
				<owl:onProperty>
					<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#extensionXML">
						<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#Snowboard"/>
						<rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"/>
					</owl:DatatypeProperty>
				</owl:onProperty>
			</owl:Restriction>
		</rdfs:subClassOf>
		
	</owl:Class>
	
	<!-- the following three properties now (correctly) get added to all the classes because
	     they are open domain properties in the same ontology -->
	
	<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#complimentBoard" />

	<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#preferredStance">
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
	</owl:DatatypeProperty>

	<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#isAlpine">
		<rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#isFreestyle">
		<rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
	</owl:ObjectProperty>
		
	<!-- End open domain properties -->
	
	<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#proRider">
	  <rdfs:domain>
	    <owl:Class>
	      <owl:unionOf rdf:parseType="Collection">
	        <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#TwinTip"/>
	        <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#PowderSki"/>
	      </owl:unionOf>
	    </owl:Class>
	  </rdfs:domain>
	  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
	</owl:DatatypeProperty>	
	
	<owl:ObjectProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#designer">
		<rdfs:domain>
		    <owl:Class>
		      <owl:unionOf rdf:parseType="Collection">
		        <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#Ski"/>
		        <owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#Snowboard"/>
		      </owl:unionOf>
		    </owl:Class>
		</rdfs:domain>
	    <rdfs:range rdf:resource="http://jastor.openanzo.org/testonts/classes#Ski"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="http://jastor.openanzo.org/testonts/predicates#specialty">
		<rdfs:domain rdf:resource="http://jastor.openanzo.org/testonts/classes#SpecialtySki"/>
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
	</owl:DatatypeProperty>	
	
	<owl:Class rdf:about="http://jastor.openanzo.org/testonts/classes#BaseColor">
		<owl:oneOf rdf:parseType="Collection">
   			<classes:BaseColor rdf:about="http://jastor.openanzo.org/testonts/individuals#Black" />
    		<classes:BaseColor rdf:about="http://jastor.openanzo.org/testonts/individuals#White" />
    		<owl:Thing rdf:about="http://jastor.openanzo.org/testonts/individuals#Grey" />
  		</owl:oneOf>
	</owl:Class>
	
	
	
</rdf:RDF>